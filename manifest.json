{
    "manifest_version": 3,
    "name": "Dashboard",
    "version": "1.0",
    "permissions": [
        "bookmarks",
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "options.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    }
}